<!DOCTYPE>
<html>
<head>
	<link rel="stylesheet" type="text/css" href="css/default.css" title="default" />
	<link rel="stylesheet" type="text/css" href="css/main.css" title="default" />
	<link rel="shortcut icon" href="img/favicon.ico" />
	<title><%= title %></title>
	<!-- jQuery -->
	<script src="js/jquery-1.3.2.min.js" type="text/javascript"></script>
	<!-- jQuery slider plugin -->
	<script src="js/easySlider1.5.js" type="text/javascript" ></script>
</head>


<body id="home">
<div id="wrapper">

	<div id="header">
		<!-- The name of your website -->
		<h1><a href="index.html"><strong>Компанія</strong>Нерухомості</a></h1>
		<!-- Tagline -->
		<p class="tagline">На благо людям</p>
		<ul>
			<!-- Navigation - the current page is highlighted with css class "current". -->
			<li><a href="/">Домівка <span class="navi-description">Домівка</span></a></li>
			<li class="current"><a href="/show">Оголошення</a></li>
			<li><a href="/service">Сервіси</a></li>
			<li><a href="about.html">Зворотній зв'язок</a></li>
			<li><a href="/registration">Реєстрація <span class="navi-description">приєднуйтесь </span></a></li>
		</ul><!-- End navigation -->

		<!-- Search -->
		<div id="searchbox">
			<form action="" id="search" method="post">
				<fieldset>
					<input type="submit" id="searchSubmit" value="Пошук" />
					<input type="text" name="searchTerm" size="20" />
				</fieldset>
			</form>
		</div><!-- End search -->
	</div><!-- End header -->

	<div id="contentArea">
		<div class="filters-block">
			<div class="left">
				<h4>Поиск объектов</h4>
			</div>


			<div id="edit-field-deal-type-wrapper" class="views-exposed-widget">
				<label for="edit-field-deal-type">Тип сделки</label>
				<div class="views-widget">
					<div class="form-item form-type-select">
						<select id="edit-field-deal-type" name="cat_type" class="form-select">
							<option selected="" value="sale">Продажа</option>
							<option value="rent">Аренда</option>
						</select>
					</div>
				</div>
			</div>

			<div id="edit-field-realty_type-wrapper" class="views-exposed-widget">
				<label for="edit-field-realty_type">Тип недвижимости</label>
				<div class="views-widget">
					<div class="form-item form-type-select">
						<select name="realty_type" id="edit-field-realty_type" class="form-select"><option value="">Все типы</option><option value="house_rent">Дом</option><option value="flat_rent">Квартира</option><option value="commerce_rent">Коммерческая недвижимость</option></select>								</div>
				</div>
			</div>

			<div id="edit-field-price-wrapper" class="views-exposed-widget">
				<label for="edit-field-price">Стоимость</label>
				<div class="views-widget">
					<div class="form-item form-type-select">
						<select name="price" id="edit-field-price" class="form-select"><option value="">Любая</option><option value="under_500">менее 500 €</option><option value="from_500_to_1000">от 500 до 1 000 €</option><option value="from_1000">от 1 000 €</option></select>								</div>
				</div>
			</div>


			<div id="edit-field-place-wrapper" class="views-exposed-widget">
				<label for="edit-field-place">Расположение</label>
				<div class="views-widget">
					<div class="form-item form-type-select form-item-field-agent-tid">
						<select name="place" id="edit-field-place" class="form-select"><option value="">Любой регион</option><option value="7">Любляна</option><option value="8">Пригород Любляны</option><option value="9">Блед</option><option value="10">Приморский регион</option></select>								</div>
				</div>
			</div>

			<!--
            <div class="views-exposed-widget views-submit-button">
                <input id="edit-submit-browse-cars" class="form-submit" type="submit" value="Искать" name="">
            </div>
            -->

		</div>
		<div>
			<%adverts.forEach(function(advert){%>
			<div class="news">
				<div class ="tittle">
					<h2 id="tittle"><%=advert.tittle%></h2>
					<h2 id="price">Price <%=advert.price + " $"%></h2>
				</div>
				<img  src="data:image/png;base64,<%=advert.image%>" border='0' title='<%=advert.tittle%>' />
				<p class="description">
					<%=advert.description%>
				</p>
				<a href='/advert?id=<%=advert.advert_id%>'> See more...</a>
			</div>
			<% });%>
		</div>
	</div><!-- End contentArea -->


	<div id="footerContent">
		<div class="footerColumn">
			<a href="http://themeforest.net/item/businexx/29634?ref=myTheme">
				<!-- Change your own image, size 275x80 -->
				<img src="img/promo3.jpg" alt="" title="" /></a>
			<!--<h4>Check out my other template</h4>-->
